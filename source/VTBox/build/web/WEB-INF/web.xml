<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>SerializeAnalysisServlet</servlet-name>
        <servlet-class>vtbox.servlets.SerializeAnalysisServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DataUploader</servlet-name>
        <servlet-class>vtbox.servlets.DataUploader</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SerializeFeatureList</servlet-name>
        <servlet-class>vtbox.servlets.SerializeFeatureList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadAnalysis</servlet-name>
        <servlet-class>vtbox.servlets.LoadAnalysis</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HeatmapImageServer</servlet-name>
        <servlet-class>vtbox.servlets.HeatmapImageServer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HeatmapDataServer</servlet-name>
        <servlet-class>vtbox.servlets.HeatmapDataServer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DoSearch</servlet-name>
        <servlet-class>vtbox.servlets.DoSearch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddDataToList</servlet-name>
        <servlet-class>vtbox.servlets.AddDataToList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteSearchServlet</servlet-name>
        <servlet-class>vtbox.servlets.DeleteSearchServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HeatmapDendrogramGenerator</servlet-name>
        <servlet-class>vtbox.servlets.HeatmapDendrogramGenerator</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AnalysisInitializer</servlet-name>
        <servlet-class>vtbox.servlets.AnalysisInitializer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AnalysisReInitializer</servlet-name>
        <servlet-class>vtbox.servlets.AnalysisReInitializer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AnalysisTerminator</servlet-name>
        <servlet-class>vtbox.servlets.AnalysisTerminator</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SerializeAnalysisServlet</servlet-name>
        <url-pattern>/SerializeAnalysisServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DataUploader</servlet-name>
        <url-pattern>/DataUploader</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SerializeFeatureList</servlet-name>
        <url-pattern>/SerializeFeatureList</url-pattern>
    </servlet-mapping>
    <jsp-config>
        <jsp-property-group>
            <url-pattern>*.jspf</url-pattern>
        </jsp-property-group>
    </jsp-config>
    <servlet-mapping>
        <servlet-name>LoadAnalysis</servlet-name>
        <url-pattern>/LoadAnalysis</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HeatmapImageServer</servlet-name>
        <url-pattern>/HeatmapImageServer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HeatmapDataServer</servlet-name>
        <url-pattern>/HeatmapDataServer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DoSearch</servlet-name>
        <url-pattern>/DoSearch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddDataToList</servlet-name>
        <url-pattern>/AddDataToList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteSearchServlet</servlet-name>
        <url-pattern>/DeleteSearchServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HeatmapDendrogramGenerator</servlet-name>
        <url-pattern>/HeatmapDendrogramGenerator</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AnalysisInitializer</servlet-name>
        <url-pattern>/AnalysisInitializer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AnalysisReInitializer</servlet-name>
        <url-pattern>/AnalysisReInitializer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AnalysisTerminator</servlet-name>
        <url-pattern>/AnalysisTerminator</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            -1
        </session-timeout>
    </session-config>
</web-app>
